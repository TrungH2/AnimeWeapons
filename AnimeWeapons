Â  Â  Â  Â  Â  Â  if layout then
Â  Â  Â  Â  Â  Â  Â  Â  frame.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 20)
Â  Â  Â  Â  Â  Â  end
Â  Â  Â  Â  end
Â  Â  Â  Â  if TabListLayout then
Â  Â  Â  Â  Â  Â  TabContainer.CanvasSize = UDim2.new(0, TabListLayout.AbsoluteContentSize.X + 20, 0, 0)
Â  Â  Â  Â  end
Â  Â  Â  Â  task.wait(0.5)
Â  Â  end
end)

